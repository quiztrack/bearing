{
  "name": "peek-ui",
  "version": "0.0.1-alpha.2",
  "description": "The solution components for React App.",
  "type": "module",
  "access": "public",
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/index.d.ts"
      ],
      "kit": [
        "./dist/kit/index.d.ts"
      ]
    }
  },
  "exports": {
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./kit": {
      "import": "./dist/kit/index.js",
      "require": "./dist/kit/index.cjs",
      "types": "./dist/kit/index.d.ts"
    },
    "./style.css": "./dist/components/index.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "echo \"Error: no test specified\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "https://github.com/quiztrack",
  "license": "MIT",
  "files": [
    "./src",
    "./dist"
  ],
  "devDependencies": {
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@storybook/addon-essentials": "^7.6.12",
    "@storybook/addon-interactions": "^7.6.12",
    "@storybook/addon-links": "^7.6.12",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.12",
    "@storybook/react": "^7.6.12",
    "@storybook/react-vite": "^7.6.12",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@vanilla-extract/css": "^1.14.1",
    "@vanilla-extract/dynamic": "^2.1.0",
    "@vanilla-extract/esbuild-plugin": "^2.3.5",
    "@vanilla-extract/vite-plugin": "^3.9.5",
    "autoprefixer": "^10.4.17",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.1",
    "postcss": "^8.4.33",
    "prettier": "3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.12",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3",
    "vite": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^17.0 || ^18.0",
    "react-dom": "^17.0 || ^18.0"
  },
  "dependencies": {
    "@eqpoqpe/classname-utils": "^0.1.1",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "tailwind-merge": "^2.2.1"
  },
  "packageManager": "pnpm@8.15.1",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
